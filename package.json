    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.10.0",
    "eslint-watch": "^3.1.4",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.2.1"
  }
}
